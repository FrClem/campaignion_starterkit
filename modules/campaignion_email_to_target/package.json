{
  "name": "campaignion-e2t-ui",
  "version": "0.0.1",
  "private": true,
  "main": "js/e2t-ui.js",
  "dependencies": {
    "vue": "^1.0.21",
    "vue-dnd": "^0.1.1",
    "vue-resource": "^0.7.0",
    "vue-strap": "^1.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^5.8.38",
    "bootstrap": "^4.0.0-alpha.2",
    "browserify": "^13.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.3",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "lodash": "^4.11.1",
    "minifyify": "^7.3.3",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-cli": "^2.5.1",
    "vue-hot-reload-api": "^1.3.2",
    "vueify": "^8.3.9",
    "vueify-insert-css": "^1.0.0",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "build-scripts": "NODE_ENV=production browserify -t vueify -e ui_src/main.js -p [minifyify --no-map] -o js/messages_widget.js",
    "compile-css-min": "node-sass -x -q --output-style compressed --include-path node_modules/bootstrap/scss css/scss/messages_widget.scss css/messages_widget.min.css",
    "post-css": "postcss -c postcss.options.json",
    "build": "npm run build-scripts && npm run compile-css-min && npm run post-css",
    "watch-scripts": "watchify -v -t vueify -e ui_src/main.js -o js/messages_widget.js",
    "watch-css": "node-sass -w -x -q --output-style expanded --include-path node_modules/bootstrap/scss css/scss/messages_widget.scss css/messages_widget.css",
    "watch-css-min": "node-sass -w -x -q --output-style compressed --include-path node_modules/bootstrap/scss css/scss/messages_widget.scss css/messages_widget.min.css",
    "watch": "npm run watch-scripts & npm run watch-css & npm run watch-css-min",
    "test": "./node_modules/karma/bin/karma start"
  },
  "license": "GPL-3.0"
}
